network:
  version: 2
  renderer: networkd
  ethernets:
    enp1s0f0:
      dhcp4: no
      addresses: [192.168.1.122/24]
      nameservers:
        addresses: [192.168.1.1,8.8.8.8]
      routes:
        - to: 0.0.0.0/0
          via: 192.168.1.1
    enp1s0f1: {}
    enp1s0f2: {}
    enp1s0f3: {}
  bridges:
    br0:
      interfaces: [enp1s0f1, enp1s0f2, enp1s0f3]
      addresses: [10.10.10.1/24]
      dhcp4: no